#1
n=int(input())
m=int(input())
k=1
for i in range(1,n+1):
    for j in range(1,m+1):
        print(k,end=' ')
        k=k+1
    print()

#2
n=int(input())
m=int(input())
k=m*n
for i in range(1,n+1):
    for j in range(1,m+1):
        print(k,end=' ')
        k=k-1
    print()

#3
n=int(input())
for i in range(n,0,-1):
    for j in range(i,0,-1):
        print('*',end=' ')
    print()
