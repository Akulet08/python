def age(n):
    if n>=18:
        return 'yes'
    else:
        return 'no'
n=int(input())
print(age(n))
    

def age(n):
    if n>=18:
        return True
    else:
         return False
a=int(input())
if age(a):
    print('yes')
else:
    print('no')


