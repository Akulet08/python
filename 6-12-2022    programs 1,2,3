#1
n=int(input())
s=0
while n!=0:
    r=n%10
    s=r+(s*10)
    n=n//10
print(s)


#2
n=int(input())
t=n
s=0
while n!=0:
    r=n%10
    s=r+(s*10)
    n=n//10
if t==s:
    print('palindrome')
else:
    print('not a palindrome')

#3
n=int(input())
t=n
s=0
while n!=0:
    r=n%10
    s=s+(r*r*r)
    n=n//10
if t==s:
    print('Armstrong')
else:
    print('not a Armstrong')
